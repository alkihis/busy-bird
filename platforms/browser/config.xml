<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lbbe.busybird" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Busy Bird</name>
    <description>
        Une superbe app pour enregistrer des oiseaux
    </description>
    <author email="louis.beranger@etu.univ-lyon1.fr">
        Romuald Marin, Émilien N'Guyen, Louis Béranger
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
